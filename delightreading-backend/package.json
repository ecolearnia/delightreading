{
  "name": "express-typescript-starter",
  "version": "0.1.0",
  "description": "A starting point for Node.js express apps with TypeScript",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/TypeScript-Node-Starter"
  },
  "author": "Bowden Kelly",
  "license": "MIT",
  "scripts": {
    "migrations-run": "ts-node ./node_modules/typeorm/cli.js migrations:run",
    "migrations-gen": "ts-node ./node_modules/typeorm/cli.js migrations:generate -n RefactorEntities",
    "start": "npm run serve",
    "build": "npm run build-ts && npm run tslint",
    "serve": "node dist/server.js",
    "start-ts": "ts-node src/server.ts",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\"  \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --verbose --runInBand",
    "test-cov": "jest --forceExit --coverage --verbose --runInBand",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "dependencies": {
    "@types/passport-jwt": "^3.0.1",
    "async": "^2.6.1",
    "bcrypt-nodejs": "^0.0.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.3",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "express-validator": "^4.3.0",
    "fbgraph": "^1.4.4",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "morgan": "^1.9.0",
    "nodemailer": "^4.6.7",
    "nuxt": "^1.4.1",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth": "^1.0.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-jwt": "^3.0.1",
    "passport-local": "^1.0.0",
    "pg": "^7.4.3",
    "pino": "^4.17.3",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "typeorm": "^0.1.21",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/async": "^2.0.49",
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/bluebird": "^3.5.18",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "0.0.33",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^4.0.3",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.10",
    "@types/jest": "^21.1.8",
    "@types/jsonwebtoken": "^7.2.7",
    "@types/lodash": "^4.14.110",
    "@types/lusca": "^1.5.0",
    "@types/morgan": "^1.7.32",
    "@types/node": "^7.0.66",
    "@types/nodemailer": "^1.3.32",
    "@types/passport": "^0.3.3",
    "@types/passport-facebook": "^2.1.8",
    "@types/passport-google-oauth": "^1.0.38",
    "@types/passport-local": "^1.0.33",
    "@types/pino": "^4.16.0",
    "@types/request": "^2.47.1",
    "@types/request-promise": "^4.1.41",
    "@types/shelljs": "^0.7.9",
    "@types/supertest": "^2.0.4",
    "@types/uuid": "^3.4.3",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "jest": "^21.2.1",
    "nodemon": "^1.17.5",
    "shelljs": "^0.7.7",
    "supertest": "^3.1.0",
    "ts-jest": "^21.2.4",
    "ts-node": "^4.0.2",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2"
  }
}
